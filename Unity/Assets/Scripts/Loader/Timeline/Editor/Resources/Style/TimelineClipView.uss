.timelineClip {
    height: 100%;
    position: absolute;
    flex-direction: row;
    border-left-color: rgb(37, 37, 37);
    border-right-color: rgb(37, 37, 37);
    border-top-color: rgb(37, 37, 37);
    border-bottom-color: rgb(37, 37, 37);
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
}

.mixLeft.timelineClip {
    border-left-width: 1px;
    border-left-color: rgba(0, 0, 0, 0);
}

.mixRight.timelineClip{
    border-right-width: 1px;
    border-right-color: rgba(0,0,0,0);
}

.selected.timelineClip {
    border-left-color: rgb(68, 192, 255);
    border-right-color: rgb(68, 192, 255);
    border-top-color: rgb(68, 192, 255);
    border-bottom-color: rgb(68, 192, 255);
}

.invalid.timelineClip {
    border-left-color: rgb(234, 62, 76);
    border-right-color: rgb(234, 62, 76);
    border-top-color: rgb(234, 62, 76);
    border-bottom-color: rgb(234, 62, 76);
}

